                    [ Inline
                        mempty
                        (Str
                           "AsciiDoc was first released in Nov 2002 by Stuart Rackham.\nIt was designed from the start to be a shorthand syntax\nfor producing professional documents like DocBook and LaTeX.")
                    ])
             ])
      ]
  }
